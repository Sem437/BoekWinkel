@model IEnumerable<BoekWinkel.Models.Winkelwagen>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.gebruikersId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BoekId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InWinkelwagen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Betaald)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.gebruikersId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BoekId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InWinkelwagen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Betaald)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.WinkelwagenId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.WinkelwagenId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.WinkelwagenId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
